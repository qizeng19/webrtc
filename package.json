  "scripts": {
    "fch:start": "rimraf dist && fch start ",
    "fch:build": "rimraf dist && fch build -s",
    "start": "rimraf dist && cross-env NODE_ENV=development webpack-dev-server",
    "build": "rimraf dist && cross-env NODE_ENV=production PUBLICK_PATH=./ webpack ",
    "build-dev": "rimraf dist && cross-env NODE_ENV=development PUBLICK_PATH=./  webpack",
    "build-ana": "rimraf dist && cross-env NODE_ENV=production IS_ANA=analyse PUBLICK_PATH=./ webpack",
    "dll:dev": "rimraf dll/development && cross-env NODE_ENV=development webpack --config bundle/webpack.config.dll.js",
    "dll:prod": "rimraf dll/production && cross-env NODE_ENV=production webpack --config bundle/webpack.config.dll.js",
    "lint": "eslint src --ext .js,.ts",
    "lintcss": "stylelint 'src/**/*.less' --fix"
  },
